	<string>Xcode.Xcode3.ProjectTemplateUnitKind</string>
	<key>Identifier</key>
	<string>com.corbin.pluginFramework</string>
	<key>Concrete</key>
	<true/>
	<key>Description</key>
	<string>This template builds a plugin framework that links against the Foundation and UIKit frameworks.</string>
	<key>Platforms</key>
	<array>
		<string>com.apple.platform.iphoneos</string>
	</array>
	<key>Ancestors</key>
	<array>
		<string>com.apple.dt.unit.frameworkBase</string>
	</array>
	<key>Project</key>
	<dict>
		<key>SharedSettings</key>
		<dict>
			<key>TARGETED_DEVICE_FAMILY</key>
			<string>1,2</string>
			<key>GCC_WARN_ABOUT_MISSING_PROTOTYPES</key>
			<string>YES</string>
			<key>GCC_WARN_UNUSED_VARIABLE</key>
			<string>YES</string>
			<key>GCC_WARN_ABOUT_RETURN_TYPE</key>
			<string>YES</string>
			<key>GCC_C_LANGUAGE_STANDARD</key>
			<string>gnu99</string>
			<key>EXCLUDED_RECURSIVE_SEARCH_PATH_SUBDIRECTORIES</key>
			<string>*.nib *.lproj *.gch (*) .DS_Store CVS .svn .git .hg *.xcodeproj *.xcode *.pbproj *.pbxproj</string>
			<key>IPHONEOS_DEPLOYMENT_TARGET</key>
			<string>latest_iphoneos</string>
		</dict>
		<key>SDK</key>
		<string>iphoneos</string>
		<key>Configurations</key>
		<dict>
			<key>Debug</key>
			<dict>
				<key>VALIDATE_PRODUCT</key>
				<string>NO</string>
				<key>GCC_OPTIMIZATION_LEVEL</key>
				<string>0</string>
				<key>GCC_PREPROCESSOR_DEFINITIONS</key>
				<string>DEBUG=1 $(inherited)</string>
				<key>GCC_SYMBOLS_PRIVATE_EXTERN</key>
				<string>NO</string>
				<key>COPY_PHASE_STRIP</key>
				<string>NO</string>
				<key>GCC_DYNAMIC_NO_PIC</key>
				<string>NO</string>
			</dict>
			<key>Release</key>
			<dict>
				<key>COPY_PHASE_STRIP</key>
				<string>YES</string>
				<key>VALIDATE_PRODUCT</key>
				<string>YES</string>
			</dict>
		</dict>
	</dict>
	<key>Targets</key>
	<array>
		<dict>
			<key>ProductType</key>
			<string>com.apple.product-type.framework</string>
			<key>TargetIdentifier</key>
			<string>plugin</string>
			<key>SharedSettings</key>
			<dict>
				<key>INSTALL_PATH</key>
				<string>$(LOCAL_LIBRARY_DIR)/Frameworks</string>
				<key>SKIP_INSTALL</key>
				<string>YES</string>
				<key>DYLIB_COMPATIBILITY_VERSION</key>
				<string>1</string>
				<key>DYLIB_CURRENT_VERSION</key>
				<string>1</string>
				<key>DYLIB_INSTALL_NAME_BASE</key>
				<string>@rpath</string>
				<key>DEFINES_MODULE</key>
				<string>YES</string>
				<key>GCC_PRECOMPILE_PREFIX_HEADER</key>
				<string>YES</string>
				<key>GCC_PREFIX_HEADER</key>
				<string>___PACKAGENAME___/PrefixHeader.pch</string>
				<key>CODE_SIGN_IDENTITY</key>
				<string>iPhone Developer</string>
			</dict>
			<key>BuildPhases</key>
			<array>
				<dict>
					<key>Class</key>
					<string>ShellScript</string>
					<key>ShellPath</key>
					<string>/bin/sh</string>
					<key>ShowEnvVarsInLog</key>
					<string>true</string>
					<key>ShellScript</key>
					<string>IPA_PATH=&quot;${PROJECT_DIR}/___VARIABLE_TargetAppName___.ipa&quot;
FRAMEWORK_PATH=&quot;${BUILT_PRODUCTS_DIR}/___PACKAGENAME___.framework&quot;
BUNDLE_IDENTIFIER=&quot;___VARIABLE_TargetAppBundleId___&quot;

APP_NAME=$(basename &quot;$IPA_PATH&quot;)
APP_NAME=&quot;${APP_NAME%.*}.app&quot;
APP_EXECUTABLE=&quot;${APP_NAME%.*}&quot;

FRAMEWORK_NAME=$(basename &quot;$FRAMEWORK_PATH&quot;)
FRAMEWORK_EXECUTABLE=&quot;${FRAMEWORK_NAME}/${FRAMEWORK_NAME%.*}&quot;

TEMP_DIR=$(mktemp -d)
unzip -qo &quot;${IPA_PATH}&quot; -d ${TEMP_DIR}/

cd ${TEMP_DIR}
mkdir -p &quot;Payload/${APP_NAME}/Frameworks&quot;
cp -r &quot;${FRAMEWORK_PATH}&quot; &quot;Payload/${APP_NAME}/Frameworks&quot;

plutil -replace CFBundleIdentifier -string ${BUNDLE_IDENTIFIER} &quot;Payload/${APP_NAME}/Info.plist&quot;
insert_dylib --all-yes @rpath/${FRAMEWORK_EXECUTABLE} &quot;Payload/${APP_NAME}/${APP_EXECUTABLE}&quot;
mv &quot;Payload/${APP_NAME}/${APP_EXECUTABLE}_patched&quot; &quot;Payload/$APP_NAME/${APP_EXECUTABLE}&quot;
chmod +x &quot;Payload/${APP_NAME}/${APP_EXECUTABLE}&quot;

rm -rf &quot;Payload/${APP_NAME}/PlugIns&quot;
rm -rf &quot;Payload/${APP_NAME}/Watch&quot;

rm -rf &quot;${BUILT_PRODUCTS_DIR}/${APP_NAME}&quot;
mv &quot;Payload/${APP_NAME}&quot; ${BUILT_PRODUCTS_DIR}
rm -rf ${TEMP_DIR}</string>
				</dict>
			</array>
		</dict>
		<dict>
			<key>Name</key>
			<string>___VARIABLE_TargetAppName___</string>
			<key>ProductType</key>
			<string>com.apple.product-type.application</string>
			<key>SharedSettings</key>
			<dict>
				<key>INFOPLIST_FILE</key>
				<string>___PACKAGENAME___/Info.plist</string>
				<key>PRODUCT_NAME</key>
				<string>___VARIABLE_TargetAppName___</string>
				<key>PRODUCT_BUNDLE_IDENTIFIER</key>
				<string>___VARIABLE_TargetAppBundleId___</string>
				<key>DEFINES_MODULE</key>
				<string>YES</string>
				<key>GCC_PRECOMPILE_PREFIX_HEADER</key>
				<string>YES</string>
				<key>CODE_SIGN_IDENTITY</key>
				<string>iPhone Developer</string>
			</dict>
			<key>BuildPhases</key>
			<array/>
			<key>Dependencies</key>
			<array>
				<string>plugin</string>
			</array>
		</dict>
	</array>
	<key>Options</key>
	<array>
		<dict>
			<key>Identifier</key>
			<string>TargetAppName</string>
			<key>Name</key>
			<string>Target App Name:</string>
			<key>Description</key>
			<string>Target App Name.</string>
			<key>Type</key>
			<string>text</string>
			<key>Placeholder</key>
			<string>AppStore</string>
		</dict>
		<dict>
			<key>Identifier</key>
			<string>TargetAppBundleId</string>
			<key>Name</key>
			<string>Target App Bundle Id:</string>
			<key>Description</key>
			<string>Target App Bundle Id.</string>
			<key>Type</key>
			<string>text</string>
			<key>Placeholder</key>
			<string>com.apple.appStore</string>
		</dict>
	</array>
	<key>Nodes</key>
	<array>
		<string>___PACKAGENAME___.h</string>
		<string>___PACKAGENAME___.m</string>
		<string>___PACKAGENAME___Header.h</string>
		<string>PrefixHeader.pch</string>
		<string>../Podfile</string>
		<string>Info.plist</string>
	</array>
	<key>Definitions</key>
	<dict>
		<key>../Podfile</key>
		<dict>
			<key>Group</key>
			<array>
				<string>Supporting Files</string>
			</array>
			<key>Path</key>
			<string>Podfile</string>
			<key>Beginning</key>
			<string>platform :ios, &apos;8.0&apos;

target &apos;___PACKAGENAME___&apos; do
	pod &apos;JRSwizzle&apos;, &apos;~&gt; 1.0&apos;
	pod &apos;fishhook&apos;, &apos;~&gt; 0.2&apos;
end</string>
			<key>End</key>
			<string></string>
		</dict>
		<key>___PACKAGENAME___.h</key>
		<dict>
			<key>Path</key>
			<string>___PACKAGENAME___.h</string>
		</dict>
		<key>___PACKAGENAME___.m</key>
		<dict>
			<key>Path</key>
			<string>___PACKAGENAME___.m</string>
		</dict>
		<key>___PACKAGENAME___Header.h</key>
		<dict>
			<key>Path</key>
			<string>___PACKAGENAME___Header.h</string>
		</dict>
		<key>PrefixHeader.pch</key>
		<dict>
			<key>Group</key>
			<array>
				<string>Supporting Files</string>
			</array>
			<key>Path</key>
			<string>PrefixHeader.pch</string>
		</dict>
		<key>Info.plist</key>
		<dict>
			<key>Group</key>
			<array>
				<string>Supporting Files</string>
			</array>
			<key>Path</key>
			<string>Info.plist</string>
